version: 'test'
modules:
  - name: mock
    source: ../plugins/mock/target/release/mock.dll
  - name: payload
    source: ../plugins/payload/target/release/carbon_payload.dll
pipeline:
  - payload:
      request:
        num: 1
    producer: true
    case:
      eq (calc "num % 2") 0: par
      eq (calc "num % 2") 1: impar
  - ref: "par"
    print: "par"
  - ref: "impar"
    print: "impar"
  - attach: ""
